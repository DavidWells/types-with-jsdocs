{
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "prebuild": "npm run generate",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "type-coverage": "type-coverage --detail --strict --at-least 90 --ignore-files 'test/*'",
    "ts-coverage": "typescript-coverage-report",
    "emit": "tsc --noEmit false --emitDeclarationOnly true",
    "generate": "npm run generate-zod && npm run generate-json-schema",
    "generate-zod": "ts-to-zod ./src/zod/autozod.d.ts ./src/zod/autozod-out.js",
    "x": "typescript-json-schema --ignoreErrors \"./src/utils/types.d.ts\" '*' -o ./src/utils/types-schema.json",
    "generate-json-schema": "typescript-json-schema --ignoreErrors \"./src/zod/autozod.d.ts\" '*' -o ./src/zod/autozod-schema.json"
  },
  "dependencies": {
    "@mantine/core": "^2.1.4",
    "@mantine/hooks": "^2.1.4",
    "@modulz/radix-icons": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jss": "^10.7.1",
    "zod": "^3.5.1"
  },
  "devDependencies": {
    "@davidwells/config-postcss": "0.0.21",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/parser": "^4.28.5",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "doxxx": "^1.0.0",
    "eslint": "^7.31.0",
    "eslint-plugin-jsdoc": "^36.0.6",
    "ts-to-zod": "^1.4.0",
    "type-coverage": "^2.18.0",
    "type-fest": "^1.2.3",
    "typescript": "^4.3.5",
    "typescript-coverage-report": "^0.6.0",
    "typescript-json-schema": "^0.50.1",
    "vite": "^2.4.4"
  }
}
