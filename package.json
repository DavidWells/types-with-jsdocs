{
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "type-coverage": "type-coverage --detail --strict --at-least 90 --ignore-files 'test/*'",
    "ts-coverage": "typescript-coverage-report",
    "emit": "tsc --noEmit false --emitDeclarationOnly true",
    "generate": "npm run generate-zod && npm run generate-json-schema",
    "generate-zod": "ts-to-zod ./src/zod/autozod.d.ts ./src/zod/autozod-out.js",
    "generate-json-schema": "typescript-json-schema \"./src/zod/autozod.d.ts\" '*' -o ./src/zod/autozod-schema.json"
  },
  "dependencies": {
    "@mantine/core": "^1.3.1",
    "@mantine/hooks": "^1.3.1",
    "@modulz/radix-icons": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jss": "^10.6.0"
  },
  "devDependencies": {
    "@davidwells/config-postcss": "0.0.20",
    "@types/react": "^17.0.9",
    "@types/react-dom": "^17.0.6",
    "@typescript-eslint/parser": "^4.28.5",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "doxxx": "^1.0.0",
    "eslint": "^7.31.0",
    "eslint-plugin-jsdoc": "^35.3.2",
    "ts-to-zod": "^1.3.0",
    "type-coverage": "^2.17.5",
    "type-fest": "^1.2.2",
    "typescript": "^4.3.2",
    "typescript-coverage-report": "^0.6.0",
    "typescript-json-schema": "^0.50.1",
    "vite": "^2.3.6"
  }
}
